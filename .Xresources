! Urxvt specifics
URxvt.perl-ext-common:  resize-font
URxvt.keysym.F11:       perl:fullscreen:switch
URxvt.keysym.C-minus:   resize-font:smaller
URxvt.keysym.C-equal:   resize-font:bigger
URxvt.keysym.C-0:       resize-font:reset
URxvt.keysym.C-question:resize-font:show

!URxvt.transparent: true
!URxvt.shading: 10

!URxvt.termName: rxvt-unicode-256color

! XClip
*.selectToClipboard: true

! Transparency
!*.alpha: 0.0

! Scroll Bar
*.scrollBar: false

! Font
*.faceName: GoMono Nerd Font Mono
*.faceSize: 15

! special
*.foreground:   #f0f0f0
*.background:   #131313
*.cursorColor:  #f0f0f0

! black
*.color0:       #131313
*.color8:       #767676

! red
*.color1:       #d73a49
*.color9:       #ff4557

! green
*.color2:       #09814A
*.color10:      #0cab63

! yellow
*.color3:       #FFB20F
*.color11:      #FED442

! blue
*.color4:       #005cc5
*.color12:      #0077ff

! magenta
*.color5:       #8921FF#6016CD
*.color13:      #ea7af4#74009D

! cyan
*.color6:       #00A5CF
*.color14:      #00ccff

! white
